  "main": "index.js",
  "scripts": {
    "test": "docker-compose up --renew-anon-volumes sleep 5 & jest --watchAll --runInBand",
    "start": "NODE_ENV=production npm run build && node build/index.js",
    "build": "rimraf ./build && tsc",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "coverage": "jest --coverage --runInBand",
    "dev": "TZ=UTC NODE_ENV=development npx nodemon",
    "migration:create": "NODE_ENV=development knex migrate:make",
    "migration:revert": "knex migrate:rollback",
    "migration:run": "NODE_ENV=development knex migrate:latest",
    "migration:up": "knex migrate:up",
    "seed:create": "knex seed:make",
    "seed:run": "NODE_ENV=production knex seed:run",
    "seed:master": "knex seed:run --specific=00-populate-mentionone-demo.ts",
    "seed:clean": "knex seed:run --specific=00-populate-mentionone-demo.ts",
    "seed:reset": "knex migrate:rollback --all && knex migrate:latest && knex seed:run --specific=00-populate-mentionone-demo.ts",
    "seed:restart": "knex seed:run --specific=00-populate-mentionone-demo.ts"
  },
